	"version": "0.2.0",
	"configurations": [
		{
			"name": "train",
			"type": "python",
			"request": "launch",
			"module": "rl_zoo3.train",
			"args": [
				"--algo",
				"mppo",
				"--gym-packages",
				"welcome_to_gym",
				"--env",
				"WelcomeTo-v0",
				"--conf-file",
				"${workspaceFolder}/conf.yaml"
			],
			"justMyCode": false
		},
		{
			"name": "Python: Module",
			"type": "python",
			"request": "launch",
			"module": "test",
			"justMyCode": false
		},
		{
			"name": "Python: Current File",
			"type": "python",
			"request": "launch",
			"program": "${file}",
			"console": "integratedTerminal",
			"justMyCode": true
		}
	]
}